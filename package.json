{
  "name": "arie",
  "version": "0.0.1",
  "description": "A lightweight React library for presenting cursor movement and interaction.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "NODE_ENV=development pnpm install",
    "build": "turbo build",
    "build:arie": "tsup src/index.ts",
    "build:web": "turbo run build --filter=arie-web...",
    "dev:web": "turbo run dev --filter=arie-web...",
    "format": "prettier --write ."
  },
  "keywords": [
    "react",
    "cursor-position",
    "user-position"
  ],
  "author": "chvndler",
  "license": "MIT",
  "packageManager": "pnpm@8.6.10",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  }
}
